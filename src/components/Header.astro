---
import Logout from "./Logout.astro";
import { LogoutForm } from "./LogoutForm";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<header
  class="w-full h-full flex justify-between items-center"
  transition:animate="none"
>
  {title}
  <LogoutForm client:load />
  <Logout />
</header>
